{
  "name": "funtypes",
  "version": "0.0.0",
  "description": "Runtime validation for static types",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "type": "module",
  "files": [
    "lib/index.js",
    "lib/index.d.ts",
    "lib/index.mjs",
    "lib/readonly.js",
    "lib/readonly.d.ts",
    "lib/readonly.mjs",
    "readonly/package.json"
  ],
  "sideEffects": false,
  "exports": {
    "./readonly": {
      "types": "./lib/readonly.d.ts",
      "default": "./lib/readonly.js"
    },
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "readonly": [
        "lib/readonly.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc --noEmit && rollup -c && api-extractor run --local && node scripts/finalize-build.js",
    "format": "node scripts/format.js",
    "test": "rollup -c && jest $([ \"$CI\" = true ] && echo --coverage || echo --watch)",
    "typecheck": "tsc --noEmit --watch",
    "minify": "terser ./lib/index.js --module --ecma 2020 --keep-classnames --keep-fnames --compress 'passes=20,inline=0,pure_getters=true,hoist_funs=true,unsafe_arrows=true,unsafe_comps=true,unsafe_math=true,unsafe_methods=true' --mangle --output ./lib/index.min.js && prettier --ignore-path '' --write lib/index.min.js"
  },
  "author": "Thomas Crockett",
  "license": "MIT",
  "devDependencies": {
    "@babel/parser": "^7.17.8",
    "@microsoft/api-extractor": "^7.47.9",
    "@types/jest": "^27.4.1",
    "jest": "^30.2.0",
    "prettier": "^3.8.0",
    "rollup": "^2.26.11",
    "rollup-plugin-prettier": "^4.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "terser": "^5.46.0",
    "ts-jest": "^29.4.6",
    "type-assertions": "^1.1.0",
    "typescript": "^5.9.3"
  },
  "keywords:": [
    "runtime",
    "type",
    "validation",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForbesLindesay/funtypes.git"
  },
  "jest": {
    "verbose": false,
    "testRegex": ".*/*.spec.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "publishConfig": {
    "provenance": true
  },
  "packageManager": "npm@11.7.0+sha512.c22099a6fff8d5b2286c2a09df5352b4858a7c0c716320f58989d60ad8b29ecf2ce6fdfe97ccb41c23ffb1272e1fa079f868487dd6b81d02a2a9e199c095a117"
}
